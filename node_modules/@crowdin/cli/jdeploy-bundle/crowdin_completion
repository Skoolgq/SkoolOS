    --base-path)
      return
      ;;
    -i|--project-id)
      return
      ;;
    --type)
      return
      ;;
    --language)
      return
      ;;
    --file)
      return
      ;;
    --workflow-step)
      return
      ;;
    --description)
      return
      ;;
    --label)
      return
      ;;
  esac

  if [[ "${curr_word}" == -* ]]; then
    COMPREPLY=( $(compgen -W "${flag_opts} ${arg_opts}" -- "${curr_word}") )
