  type compopt &>/dev/null && compopt +o default

  case ${prev_word} in
    -T|--token)
      return
      ;;
    --base-url)
      return
      ;;
    --base-path)
      return
      ;;
    -i|--project-id)
      return
      ;;
    -s|--source)
      return
      ;;
    -t|--translation)
      return
      ;;
    --dest)
      return
      ;;
    -c|--config)
      local IFS=$'\n'
      type compopt &>/dev/null && compopt -o filenames
