      ;;
    -T|--token)
      return
      ;;
    --base-url)
      return
      ;;
    --base-path)
      return
      ;;
    --id)
      return
      ;;
    --name)
      return
      ;;
    --language)
      return
      ;;
    --scheme)
      return
      ;;
  esac

  if [[ "${curr_word}" == -* ]]; then
    COMPREPLY=( $(compgen -W "${flag_opts} ${arg_opts}" -- "${curr_word}") )
  else
    local positionals=""
    local currIndex
    currIndex=$(currentPositionalIndex "upload" "${arg_opts}" "${flag_opts}")
    if (( currIndex >= 0 && currIndex <= 0 )); then
