  local curr_word=${COMP_WORDS[COMP_CWORD]}
  local prev_word=${COMP_WORDS[COMP_CWORD-1]}

  local commands="translations sources"
  local flag_opts="-V --version -h --help --no-progress -v --verbose --no-colors --preserve-hierarchy --delete-obsolete --no-auto-update --dryrun --tree --plain"
